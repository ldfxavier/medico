@media print
    #menu_topo, #busca_principal, #menu_principal, #bg_topo, i, .controlador, .bloco_controlador, .print_hide
        display: none !important
    #conteudo_principal
        width: calc(100% - 20px) !important
        margin-top: 0 !important
    .lista_dados
        width: 100% !important
        margin: 0 !important
        .topo li *
            color: #000 !important
        .topo li span
            display: none !important
        li
            background: #FFF !important
            border-top: 1px solid #000 !important
            border-radius: 0 !important
    .app_geral_lista ul.ul_lista.lista li:last-child
        border-bottom: 1px solid #000 !important
    .header_principal
        padding: 0 !important
        background-color: #FFF !important
        h1
            padding: 0 !important
            margin: 0 !important
            color: #000 !important

    .td.status
        span
            width: 100% !important
            line-height: 26px !important
            border: none !important
            background-color: #FFF !important
            border-radius: 0 !important
            overflow: hidden
            &::after
                content:attr(data-ajuda)
