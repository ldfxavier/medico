.app_visualizar_noticia .header
    width: 100%
    text-align: center
    margin-top: 20px

.app_visualizar_noticia .header h1
    width: 100%
    line-height: 1.5em
    font-size: 3.0em
    font-weight: bold

.app_visualizar_noticia .header p
    width: 100%
