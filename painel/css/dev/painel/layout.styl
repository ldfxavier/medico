    max-width: 500px
    border-radius: 4px
    overflow: hidden
    display: flex
    flex-direction: column
    .header_principal
        border-bottom: 1px solid #EEE
    form
        padding: 0 20px 20px 20px
        background-color: #FFF
    form fieldset
        border: none
        padding: 0
        margin-bottom: 10px
        border-bottom: 1px solid #DDD
        box-shadow: none
        border-radius: 0
    form input[type=number],
    form input
        width: 100%
        max-width: 100%
    form .bloco_botao
        width: 100%
        display: flex
        flex-direction: row
    form button
        height: 40px
        position: relative
        padding: 0 20px 0 60px
        color: #FFF
        background-color: $cor_add
        border-radius: 3px
    form button i
        width: 38px
        height: 38px
        line-height: 38px
        position: absolute
        top: 0
        left: 0
        text-align: center
        font-size: 1.4em
        background-color: rgba(0,0,0,.1)
    form button.deletar
        background-color: rgba(0,0,0,0)
        border: 1px solid #999
        color: #666
    form button.deletar i
        background-color: rgba(0,0,0,0)
        border-right: 1px solid #999



/* visualizar geral*/
.app_geral_visualizar
    width: 100%
    padding-bottom: 40px
    .center
        width: calc(100% - 40px)
        max-width: 1000px
        margin: 0 auto
        display: flex
        flex-direction: row
        flex-wrap: wrap
    figure.capa_perfil
        width: 100%
        height: 200px
        margin-bottom: 100px
        background-color: #FAFAFA
        padding-top: 120px
    figure.capa_perfil span
        width: 160px
        height: 160px
        margin: 0 auto
        border-radius: 100%
        background-color: #FFF
        border: 1px solid #FAFAFA
        display: table
    figure.perfil
        width: 100%
        height: 300px
        position: relative
        background-color: #FFF
        background-size: cover
        background-repeat: no-repeat
        background-position: center center
        box-shadow: 0 2px 2px #CCC
        border-radius: 3px
    .dados_esquerda
        width: 300px
        margin: 20px 20px 0 0
    .dados_direita
        width: calc(100% - 320px)
    article.bloco
        width: 100%
        margin-top: 20px
        padding: 10px 20px 10px 5px
        background-color: #FFF
        box-shadow: 0 2px 2px #CCC
        border-radius: 3px
        display: flex
        flex-direction: column
    article.bloco:last-child
        border: none
    article.bloco header
        width: 100%
        padding: 5px 10px 5px 6px
        margin-bottom: 5px
        border-left: 4px solid $painel
        font-weight: bold
    article.bloco p
        width: 100%
        padding: 5px 10px
        font-size: 1.3em
        display: flex
        flex-direction: row
        align-items: center
    article.bloco p span
        margin-right: 5px
    article.bloco p a.link_borda
        border: 1px solid $cor_editar
        color: $cor_editar
        padding: 3px 5px
        border-radius: 2px
    article.bloco p .operadora
        background-color: $azul
        color: #FFF
        padding: 5px 7px 5px 6px
        border-radius: 3px
        cursor: default
    article.bloco p.status_texto span:first-child,
    article.bloco p.status_icone span:first-child
        height: 20px
        line-height: 20px
        margin-right: 5px
    article.bloco p.status_icone span:last-child
        width: 20px
        height: 20px
        line-height: 18px
        text-align: center
        font-family: painel
        color: $verde
        border: 1px solid $verde
        border-radius: 100%
        cursor: default
    article.bloco p.status_icone span.status_1
        width: 20px
        height: 20px
        line-height: 20px
        color: $verde
        border: 1px solid $verde
    article.bloco p.status_icone span.status_2
        width: 20px
        height: 20px
        line-height: 20px
        color: $vermelho
        border: 1px solid $vermelho
    article.bloco p.status_icone span.status_1:after
        content: "\e80b"
    article.bloco p.status_icone span.status_2:after
        content: "\e813"
    article.bloco p.status_texto span:last-child
        height: 20px
        line-height: 18px
        padding: 0 5px
        border: 1px solid $verde
        border-radius: 3px
    article.endereco
        width: 100%
        margin-top: 20px
        padding-bottom: 20px
        border-bottom: 1px solid #DDD
        padding: 10px 20px 10px 5px
        background-color: #FFF
        box-shadow: 0 2px 2px #CCC
        border-radius: 3px
    article.endereco:last-child
        border: none
    article.endereco header
        width: 100%
        padding: 5px 10px 5px 6px
        margin-bottom: 5px
        border-left: 4px solid $painel
        font-weight: bold
    article.endereco p
        width: 100%
        position: relative
        padding: 28px 10px 10px 35px
        font-size: 1.3em
    article.endereco p.zero
        padding: 10px
    article.endereco p span
        width: 100%
        height: 17px
        line-height: 17px
        position: absolute
        top: 5px
        left: 10px
        font-size: 1.2em
    article.endereco p a
        width: 20px
        height: 20px
        line-height: 20px
        position: absolute
        top: 26px
        left: 10px
        font-size: .8em
        text-align: center
        background-color: $azul
        color: #FFF
        border-radius: 2px
    article.endereco p a i
        width: 100%
        height: 100%
    .bloco_quadrado
        width: 100%
        margin-top: 15px
        display: flex
        flex-direction: row
        flex-wrap: wrap
        justify-content: space-between
    .bloco_quadrado li
        width: calc(50% - 5px)
        text-align: center
        margin-top: 5px
        border: 1px solid #DDD
        display: flex
        flex-direction: column
    .bloco_quadrado li a
        padding: 10px 0
    .bloco_quadrado li span
        width: 100%
        font-size: 3.0em
        font-weight: bold
        color: $vermelho2
    .bloco_quadrado li p
        width: 100%
        font-size: 1.6em
        color: #999
    .bloco_lista
        width: 100%
        margin: 20px 0
        background-color: #FFF
        box-shadow: 0 2px 2px #CCC
        border-radius: 2px
    .bloco_lista header
        width: 100%
        height: 40px
        border-bottom: 1px solid #EEE
        display: flex
        flex-direction: row
    .bloco_lista header h1
        height: 40px
        line-height: 40px
        padding: 0 10px
        flex-grow: 1
    .bloco_lista header a.abrir
        width: 30px
        height: 30px
        line-height: 27px
        position: relative
        text-align: center
        border: 1px solid #DDD
        color: #000
        border-radius: 2px
        margin: 4px 5px 0 0
        cursor: pointer
    .bloco_lista header a i
        width: 100%
        height: 100%
    .bloco_lista ul li
        width: 100%
        padding: 10px
        font-size: 1.2em
        cursor: default
        overflow: hidden
    .bloco_lista ul li.zero
        height: 300px
        line-height: 300px
        text-align: center
        color: #999
    .album
        width: 100%
        position: relative
        padding: 20px 0 0 20px
        display: flex
    .album .add
        width: 100%
        max-width: 300px
        height: 250px
        position: absolute
        top: 20px
        left: 20px
        box-shadow: 0 2px 2px #CCC
        border-radius: 3px
        cursor: pointer
        display: flex
        flex-direction: column
        justify-content: center
        align-items: center
        background-color: #FFF
        overflow: hidden
    .album .add input
        width: 100%
        height: 100%
        position: absolute
        top: 0
        left: 0
        z-index: 3
        opacity: 0
    .album .add i
        width: 70px
        height: 70px
        line-height: 70px
        position: relative
        text-align: center
        margin-bottom: 10px
        font-size: 2.4em
        color: #999
        border: 1px solid #DDD
        border-radius: 100%
        transition: color .3s ease-out
        z-index: 2
        &::after
            font-family: painel
            content: "\f0ee"
    .album .add p
        width: 100%
        position: relative
        text-align: center
        font-size: 1.4em
        font-weight: bold
        color: #999
        transition: color .3s ease-out, border-color .3s ease-out
        z-index: 2
    .album .add span
        width: 100%
        height: 100%
        position: absolute
        top: 100%
        left: 0
        background-color: $cor_add
        transition: top .15s ease-out
        z-index: 1
    .album .add.hover i,
    .album .add:hover i
        border-color: #FFF
        color: #FFF
    .album .add.hover p,
    .album .add:hover p
        color: #FFF
    .album .add.hover span,
    .album .add:hover span
        top: 0
    .album .lista
        width: 100%
        display: flex
        flex-direction: row
        flex-wrap: wrap
    .album .lista .imagem
        width: 100%
        max-width: 300px
        height: 250px
        position: relative
        margin: 0 20px 20px 0
        background-color: #FFF
        background-size: cover
        background-position: center center
        overflow: hidden
    .album .lista .imagem i
        width: 40px
        height: 40px
        line-height: 40px
        position: absolute
        top: 10px
        text-align: center
        color: #FFF
        font-size: 1.4em
        cursor: pointer
        opacity: 0
        transition: opacity .3s ease-out
    .album .lista .imagem:hover i
        opacity: 1
    .album .lista .imagem i.editar
        background-color: $cor_editar
        right: 10px
    .album .lista .imagem i.capa
        background-color: $verde
        right: 60px
    .album .lista .imagem i.deletar
        background-color: $cor_deletar
        left: 10px
    .bloco_historico
        margin-top: 20px
@media screen and (max-width: 800px)
    .app_geral_visualizar
        .center
            display: flex
            flex-direction: column
            align-items: center
        .dados_esquerda
            width: 300px
            height: 300px
            margin: 40px 0
            border-radius: 50%
            overflow: hidden
        .dados_direita
            width: 100%
/* mobile */
#conteudo_site.mobile .app_geral_visualizar
    padding-bottom: 20px
    .bloco_historico
        width: calc(100% - 290px)
        margin-left: 270px
    .center
        width: 100%
        max-width: 100%
        display: flex
        flex-direction: row
        align-items: flex-end
        .dados_esquerda
            width: 250px
            height: calc(100vh - 50px)
            top: 50px
            left: 0
            position: fixed
            border-radius: 0
        .dados_esquerda figure
            height: 100%
        .dados_direita
            width: calc(100% - 250px)
            margin: 0 0 0 250px
            padding: 0 20px
@media screen and (max-width: 600px)
    #conteudo_site.mobile .app_geral_visualizar
        .bloco_historico
            width: calc(100% - 40px)
            margin-left: 20px
        .center
            width: 100%
            max-width: 100%
            display: flex
            flex-direction: column
            align-items: flex-start
        .dados_esquerda
            width: 100%  !important
            height: 300px !important
            top: 50px
            left: 0
            position: fixed
            border-radius: 0
        .dados_esquerda figure
            height: 300px
        .dados_direita
            width: 100% !important
            padding: 0 20px
            margin: 290px 0 0 0 !important
            z-index: 2  !important
            background-color: #FEFEFE

.bloco_agenda_add
    width: calc(100% - 40px)
    max-width: 400px
    background-color: #FFF
    border-radius: 4px
    overflow: hidden
    header
        width: 100%
        height: 50px
        line-height: 50px
        border-radius: 4px 4px 0 0
        display: flex
        border-bottom: 1px solid #EEE
        align-items: center
        flex-direction: row
    header h1
        flex-grow: 1
        color: #999
    header a.voltar
        width: 50px
        height: 50px
        line-height: 50px
        font-size: 1.2em
        text-align: center
        font-family: painel
        color: #999
        &:after
            width: 30px
            height: 30px
            content: "\e815"
    .contato_zero
        width: 100%
        background-color: #FFF
        font-size: 1.4em
        text-align: center
        padding: 70px 40px
    .contato_zero h2
        color: $vermelho2
        margin-bottom: 5px
    .contato_zero span
        color: #666
    fieldset
        width: 100%
        padding: 20px 0 10px 20px
        display: flex
        flex-direction: column
    fieldset .input_data
        width: 100%
        display: flex
        flex-direction: row
        flex-wrap: wrap
    fieldset .input_data label
        width: 100%
    fieldset .input_data input
        width: calc(100% - 80px)
    fieldset .input_data input[name=hora]
        width: 70px
        text-align: center
    fieldset textarea
        width: calc(100% - 20px)
    footer
        width: 100%
        position: relative
        padding: 10px 20px
        border-top: 1px solid #EEE
        border-radius: 0 0 4px 4px
        display: flex
        justify-content: flex-end
    footer .botao
        height: 40px
        line-height: 40px
        padding: 0 30px 0 25px
        border-radius: 3px
        font-size: 1.2em
        background-color: $verde
        color: #FFF
        cursor: pointer

#bloco_agenda_realizar
    width: calc(100% - 40px)
    max-width: 700px
    position: relative
    background-color: #FFF
    display: flex
    flex-direction: column
    border-radius: 4px
    overflow: hidden
    header.header_principal
        border-bottom: 1px solid #EEE
    .conteudo
        width: 100%
        padding: 20px
        text-align: center
        display: flex
        flex-direction: column
        align-items: center
        justify-content: center
    .conteudo .titulo
        color: $verde
        font-weight: bold
        font-size: 2.4em
    .conteudo .texto
        line-height: 1.5em
        font-size: 1.4em
    .conteudo time.agendamento
        width: 100%
        margin: 20px 0 5px 0
        display: flex
        flex-direction: column
    .conteudo time.agendamento p
        font-size: 1.2em
    .conteudo time.agendamento span
        font-size: 1.4em
        margin-top: 5px
    .conteudo .status
        width: 100%
        display: flex
        flex-direction: column
        justify-content: center
        align-items: center
    .conteudo .status span
        height: 20px
        line-height: 18px
        padding: 0 8px
        color: #FFF
        font-size: 1.4em
        border-radius: 10px
        &:nth-child(2)
            background-color: $azul
            display: none
        &:nth-child(3)
            background-color: $verde
            display: none
        &:nth-child(4)
            background-color: $vermelho2
            display: none
    .conteudo.ligando .status span
        &:nth-child(1)
            display: none
        &:nth-child(2)
            display: block
    .conteudo.finalizado .status span
        &:nth-child(1)
            display: none
        &:nth-child(3)
            display: block
    .conteudo.cancelado .status span
        &:nth-child(1)
            display: none
        &:nth-child(4)
            display: block
    .conteudo .valor
        width: 100%
        position: relative
        margin-top: 20px
        display: flex
        flex-direction: column
    .conteudo .valor p
        width: 100%
        font-size: 1.2em
    .conteudo .valor span
        width: 100%
        font-size: 2.0em
        color: #999
        margin-top: 5px
    .conteudo .link
        height: 25px
        line-height: 25px
        position: relative
        margin-top: 10px
        color: #FFF
        background-color: $cor_editar
        display: flex
        border-radius: 3px
        overflow: hidden
    .conteudo .link i
        width: 25px
        height: 25px
        background-color: rgba(0,0,0,.1)
        font-size: 1.6em
    .conteudo .link span
        font-size: 1.3em
        padding: 0 10px
    .conteudo .ligar
        width: 100px
        height: 100px
        line-height: 100px
        background-color: $verde5
        color: #FFF
        margin-top: 40px
        text-align: center
        font-size: 2.4em
        border-radius: 50%
        font-family: painel
        &::after
            content: "\e818"
    .conteudo.finalizado .ligar,
    .conteudo.cancelado .ligar
        display: none
    .conteudo.ligando .ligar
        background-color: $vermelho2
    .conteudo .cancelar
        margin-top: 20px
        padding: 20px 30px
        border-radius: 3px
        font-size: 1.2em
        border: 1px solid #CCC
        color: #999
        &:hover
            border-color: $vermelho2
            color: $vermelho2
    .conteudo.cancelado .cancelar,
    .conteudo.finalizado .cancelar,
    .conteudo.ligando .cancelar
        display: none
    .conteudo form
        width: 100%
        display: flex
        flex-direction: column
        margin-top: 40px
    .conteudo form.form_telefone
        display: none
    .conteudo.ligando form.form_telefone,
    .conteudo.finalizado form.form_telefone
    .conteudo.cancelado form.form_telefone
        display: flex
    .conteudo form textarea
        width: 100%
        height: 150px
        border: 1px solid #CCC
        padding: 10px
        font-size: 1.4em
    .conteudo form .bloco_remarcar
        width: 100%
        margin-top: 10px
        display: flex
        flex-direction: row
        background: #EEE
        padding: 5px 10px
        display: none
    .conteudo form .bloco_remarcar p
        height: 40px
        line-height: 40px
        font-size: 1.4em
    .conteudo form .bloco_remarcar .daqui_hora
        height: 20px
        line-height: 20px
        padding: 0 10px
        margin: 10px 0 0 10px
        background-color: $cor_add
        color: #FFF
        border-radius: 3px
    .conteudo form .bloco_remarcar input
        height: 40px
        padding: 0 10px
        font-size: 1.4em
        text-align: center
        margin-left: 10px
        background-color: #FFF
    .conteudo form .bloco_remarcar input[name=data]
        width: 100px
    .conteudo form .bloco_remarcar input[name=hora]
        width: 60px
    .conteudo form .bloco_botao
        width: 100%
        margin-top: 10px
        display: flex
        flex-direction: row
    .conteudo form .bloco_botao .botao
        padding: 15px 25px
        background-color: #999
        border-radius: 2px
        cursor: pointer
        font-size: 1.1em
        margin-right: 10px
        color: #FFF
    .conteudo form .bloco_botao .botao.but_cancelar.ativo
        background-color: $vermelho2
    .conteudo form .bloco_botao .botao.but_finalizar.ativo
        background-color: $verde
    .conteudo form .bloco_botao button.botao
        margin-right: 0
        background-color: $cor_add
    .conteudo .historico
        width: 100%
        margin-top: 40px
        padding-top: 10px
        border-top: 1px solid #EEE
        display: flex
        flex-direction: row
    .conteudo .historico i
        width: 40px
        height: 40px
        line-height: 40px
        text-align: center
        font-size: 1.4em
        border: 1px solid #CCC
        justify-content: flex-start
        align-items: flex-start
        border-radius: 3px
    .conteudo .historico .conteudo_dados
        width: calc(100% - 40px)
        padding-left: 10px
        display: flex
        flex-direction: column
        text-align: center
    .conteudo .historico .conteudo_dados .nome
        width: 100%
        color: #666
        font-size: 1.6em
        text-align: left
        margin: 0
        padding: 0
    .conteudo .historico .conteudo_dados time
        width: 100%
        text-align: left
        font-size: 1.2em
        color: #999
    .conteudo .historico .conteudo_dados p
        width: 100%
        line-height: 1.5em
        text-align: left
        font-size: 1.4em
        color: #333


