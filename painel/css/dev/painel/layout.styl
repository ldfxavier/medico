@import "../geral/resetar.styl"
@import "../geral/variaveis.styl"
@import "../geral/mixins.styl"
@import "../geral/plugins.styl"

fonte(painel, 1)

/* TAG / COMECO */
body
    font-family: 'Roboto', Arial, sans-serif
    background-color: #F1F6F9
.center
    width: calc(100% - 40px)
    max-width: 1000px
    margin: 0 auto
i:after
    width: 100%
    height: 100%
    text-align: center
    content: attr(data-font)
    font-family: painel
*[data-admin]
    position: relative
*[data-admin]:after
    width: 100%
    height: 30px
    position: absolute
    content: " "
    top: 0
    left: 0
    z-index: 10
/* TAG / FINAL */

/* TEMPLATE / COMECO */
#conteudo_site
    width: 100%
    display: flex
    flex-direction: column
    flex-wrap: nowrap
    align-items: flex-end
#conteudo_site.mobile .print
    display: none
/* menu do topo */
#menu_topo
    width: 40px
    height: 40px
    line-height: 40px
    position: fixed
    top: 5px
    left: 5px
    text-align: center
    color: #FFF
    font-size: 20px
    cursor: pointer
    z-index: 3
/* bg do topo */
#bg_topo
    width: 100%
    height: 170px
    position: fixed
    background-color: $painel
    z-index: 1
/* buscar principal */
#busca_principal
    width: 100%
    height: 50px
    position: fixed
    top: 0
    right: 0
    padding-left: 240px
    background-color: $painel
    z-index: 2
/* menu principal */
#conteudo_site.aberto #menu_principal
    left: 0
#conteudo_site.fechado #menu_principal
    left: -100%
#menu_principal
    width: 100%
    max-width: 240px
    height: calc(100vh - 50px)
    position: fixed
    top: 50px
    z-index: 4
    .conteudo
        width: 100%
        display: flex
        flex-direction: row
        flex-wrap: wrap
        background-color: $painel
    header
        width: 100%
        height: 120px
        padding: 10px
        display: flex
        flex-direction: row
        flex-wrap: wrap
        justify-content: space-between
    header figure
        width: 70px
        height: 70px
        display: flex
    header figure a
        width: 100%
        height: 100%
        background-color: #FFF
        background-size: cover
        background-position: center center
        border-radius: 100%
        overflow: hidden
    header .dados_linha
        width: calc(100% - 80px)
        color: #FFF
        display: flex
        flex-direction: column
    header .dados_linha .texto
        width: 100%
        color: #FFF
    header .dados_linha .nome
        width: 100%
        height: 2.2em
        line-height: 1.1em
        margin-top: 4px
        font-weight: bold
        font-size: 1.8em
        color: #FFF
    header .dados_linha .botao
        margin-top: 10px
        display: flex
        flex-direction: row
        justify-content: space-between
    header .dados_linha .botao a
        width: 30px
        height: 30px
        line-height: 30px
        text-align: center
        color: #FFF
        text-align: center
        font-size: 1.1em
        border: 1px solid rgba(255,255,255,.4)
    header .dados_linha .botao a i
        width: 30px
        height: 30px
    a.dashboard
        width: 100%
        height: 60px
        line-height: 60px
        position: relative
        padding-left: 40px
        color: $painel
        background-color: rgba(0,0,0,.1)
        color: #FFF
        font-size: 1.2em
    a.dashboard i
        width: 40px
        height: 60px
        line-height: 55px
        position: absolute
        font-weight: normal
        top: 0
        left: 0
        font-size: 1.2em
        text-align: center
    ul.menu_principal
        width: 100%
        height: calc(100vh - 230px)
        padding-top: 10px
        background-color: #FFF
        overflow-y: auto
    ul.menu_principal > li
        width: 100%
        margin-bottom: 2px
        display: flex
        flex-direction: column
    ul.menu_principal > li a.simples
        width: 100%
        height: 50px
        line-height: 50px
        position: relative
        padding: 0 10px 0 15px
        font-size: 1.1em
        color: #666
        cursor: pointer
    ul.menu_principal > li a.simples.hover,
    ul.menu_principal > li a.simples:hover
        background-color: #F4F4F4
    ul.menu_principal > li a > span.alerta
        width: 20px
        height: 20px
        line-height: 20px
        position: absolute
        top: 15px
        right: 35px
        text-align: center
        background-color: $vermelho
        color: #FFF
        border-radius: 100%
    ul.menu_principal .menu_titulo
        width: 100%
        height: 50px
        line-height: 50px
        position: relative
        padding: 0 10px 0 15px
        font-size: 1.1em
        color: #666
        cursor: pointer
    ul.menu_principal .menu_titulo i
        height: 50px
        line-height: 50px
        font-weight: normal
        position: absolute
        top: 0
        right: 20px
    ul.menu_principal .menu_titulo > span.alerta
        width: 20px
        height: 20px
        line-height: 20px
        position: absolute
        top: 15px
        right: 35px
        text-align: center
        background-color: $vermelho2
        color: #FFF
        border-radius: 100%
    ul.menu_principal .menu_titulo.hover,
    ul.menu_principal .menu_titulo:hover
        background-color: #F4F4F4
    ul.menu_principal li ul
        width: 100%
        display: none
        &.hover
            display: block
    ul.menu_principal li ul li a
        width: 100%
        height: 40px
        line-height: 40px
        position: relative
        padding: 0 20px 0 30px
        font-size: 1.2em
        color: #000
        display: flex
        &.hover,
        &:hover
            background-color: #F4F4F4
    ul.menu_principal li ul li a i
        width: 10px
        height: 40px
        line-height: 37px
        position: absolute
        top: 0
        left: 20px
    ul.menu_principal li ul li a span.alerta
        width: 20px
        height: 20px
        line-height: 20px
        position: absolute
        top: 10px
        right: 10px
        text-align: center
        background-color: $vermelho2
        color: #FFF
        border-radius: 100%
        overflow: hidden
/*menu mobile */
ex_menu()
    width: 100%
    max-width: 100%
    height: 100vh
    top: 0
    background-color: rgba(0,0,0,.7)
    z-index: 99999999920
ex_menu_cont()
    max-width: 240px
    height: 100%
ex_menu_ul()
    height: calc(100vh - 180px)
#conteudo_site.mobile.aberto #menu_principal
    ex_menu()
    .conteudo
        ex_menu_cont()
    .conteudo ul.menu_principal
        ex_menu_ul()
@media screen and (max-width: 1100px)
    #conteudo_site.aberto #menu_principal
        ex_menu()
        .conteudo
            ex_menu_cont()
        ul.menu_principal
            ex_menu_ul()


/* conteudo principal */
#conteudo_principal
    min-height: calc(100vh - 50px)
    position: relative
    margin: 50px 0 0 0
    background-color: #F1F6F9
    z-index: 3
    overflow: hidden
#conteudo_site.aberto #conteudo_principal
    width: calc(100% - 250px)
    border-radius: 5px 5px 0 0
    margin: 50px 10px 0 0
    z-index: 3
    header.header_principal_fixed
        width: calc(100% - 250px)
        top: 0
        right: 10px
#conteudo_site.fechado #conteudo_principal
    width: 100%
    border-radius: 0
    margin: 50px 0 0 0
    z-index: 1
@media screen and (max-width: 1100px)
    #conteudo_site.aberto #conteudo_principal
        width: 100%

/* TEMPLATE / FINAL */

/* CLASSES / COMECO */
.zero_ok
    width: 100%
    padding: 100px 0
    display: flex
    flex-direction: column
    align-items: center
    i
        width: 100px
        height: 100px
        line-height: 100px
        color: #CCC
        border: 1px solid #CCC
        border-radius: 50%
        font-size: 3.0em
        text-align: center
    span
        margin-top: 30px
        font-size: 1.5em
        color: #999
.zero_geral
    width: 100%
    padding: 100px 0
    font-size: 1.5em
    color: #999
    text-align: center
/*Cores */
.bg_cor
    background-color: $painel
.cor
    color: $painel
.border_cor
    border-color: $painel
.hide
    display: none !important
/* editor de texto */
.textarea_editor_grande
    width: 100%
    height: 100px
    position: relative

/* titulos */
.titulo
    width: 100%
    font-size: 1.8em
.titulo_sub
    width: 100%
    font-size: 1.4em
.texto
    width: 100%
    font-size: 1.2em
.titulo_app
    width: 100%
    font-size: 1.6em
    font-weight: bold
    text-align: center
    padding-top: 40px
.bold
    font-weight: bold
.flex-grow
    flex-grow: 1
.flex-column
    display: flex
    flex-direction: column
.flex-row
    display: flex
    flex-direction: row
.flex-wrap
    display: flex
    flex-wrap: wrap
/* esconde os inputs */
input.input_zero
    esconder()

/* Pede senha */
.bloco_admin
    width: auto
    position: relative
    display: flex
    flex-direction: column
    span
        width: 100%
        height: 100%
        position: absolute
        top: 0
        left: 0
        z-index: 999
        cursor: pointer
/* formulario geral */
form.form_geral
    width: 100%
    display: flex
    flex-direction: column
    p.ate
        height: 40px
        line-height: 40px
        margin: 0 10px 0 -10px
        font-size: 1.3em
    label
        width: 100%
        font-size: 1.4em
    input[type=password],
    input[type=number],
    input[type=email],
    input[type=tel],
    input[type=text],
    input[type=url]
        width: calc(100% - 20px)
        height: 40px
        margin: 0 20px 10px 0
        padding: 0 10px
        border: 1px solid #DDD
        font-size: 1.4em
        border-radius: 2px
    /* textarea */
    textarea
        width: 100%
        height: 9em
        line-height: 1.5em
        margin: 0 20px 10px 0
        padding: 10px
        border: 1px solid #DDD
        font-size: 1.4em
        border-radius: 2px
    /* checkbox */
    .checkbox
        width: 100%
        display: flex
        flex-direction: row
        flex-wrap: wrap
    .checkbox input[type=checkbox]
        display: none
    .checkbox label.checkbox
        width: 100%
        max-width: 200px
        position: relative
        cursor: pointer
        display: flex
        margin: 0 10px 10px 0
        display: flex
        flex-direction: row
        flex-wrap: nowrap
        align-items: center
        overflow: hidden
    .checkbox label.checkbox span
        width: calc(100% - 25px)
    .checkbox label.checkbox::before
        width: 20px
        height: 20px
        line-height: 20px
        text-align: center
        font-family: painel
        content: " "
        margin-right: 5px
        color: #FFF
        background-color: #DDD
        border-radius: 100%
    .checkbox input:checked + label.checkbox::before
        background-color: $verde
        content: "\e80b"
    /* booleano */
    .input_booleano_geral
        width: auto
        margin-bottom: 10px
        cursor: pointer
        align-items: center
        display: flex
    .input_booleano_geral .booleano_titulo
        height: 30px
        line-height: 30px
        margin-right: 10px
        font-size: 1.3em
        overflow: hidden
    .input_booleano_geral .booleano_botao
        width: 35px
        height: 10px
        border-radius: 10px
        display: flex
    .input_booleano_geral.ativo .booleano_botao
        background-color: #90C5F5
        justify-content: flex-end
    .input_booleano_geral.inativo .booleano_botao
        background-color: #DDD
        justify-content: flex-start
    .input_booleano_geral .booleano_botao .booleano_bola
        width: 22px
        height: 22px
        margin-top: -6px
        border-radius: 100%
        box-shadow: 0 0 2px #CCC
    .input_booleano_geral.ativo .booleano_botao .booleano_bola
        background-color: #1490E9
    .input_booleano_geral.inativo .booleano_botao .booleano_bola
        background-color: #EEE
    /* url */
    .input_url_geral
        width: 100%
        display: flex
        flex-direction: row
    .input_url_geral select
        width: 100px
        max-width: 100px
        background-color: #18718b
        color: #FFF
        padding: 0 10px
        margin: 0
        border-right: none
    .input_url_geral input
        max-width: 350px
        margin-right: 0
        flex-grow: 1
    /* password */
    .input_password_geral
        width: 100%
    .input_password_geral.completo
        display: flex
        flex-direction: row
        justify-content: space-between
    .input_password_geral .bloco_password
        width: 100%
        max-width: 100%
        position: relative
    .input_password_geral.completo .bloco_password
        width: calc(50% - 5px)
    .input_password_geral .bloco_password input
        width: 100%
        max-width: 100%
        padding: 0 50px 0 10px
    .input_password_geral .bloco_password .but_password_visualizar
        width: 40px
        height: 40px
        position: absolute
        top: 0
        right: 0
        cursor: pointer
        display: flex
    .input_password_geral .bloco_password .but_password_visualizar:after
        width: 40px
        height: 40px
        line-height: 40px
        color: #FFF
        font-size: 1.3em
        text-align: center
        font-family: painel
        border-radius: 0 2px 2px 0
    .input_password_geral .bloco_password .but_password_visualizar[data-password=ativo]:after
        background-color: $verde5
        content: "\e802"
    .input_password_geral .bloco_password .but_password_visualizar[data-password=inativo]:after
        background-color: $vermelho
        content: "\e806"
    .input_password_geral .bloco_password .bloco_verificar
        width: 40px
        height: 40px
        line-height: 40px
        position: absolute
        top: 0
        right: 0
        font-size: 1.4em
        text-align: center
        cursor: pointer
        display: flex
    .input_password_geral .bloco_password .bloco_verificar:after
        width: 40px
        height: 40px
        font-family: painel
        color: #FFF
        border-radius: 0 2px 2px 0
    .input_password_geral .bloco_password .bloco_verificar.correto:after
        content: "\e80b"
        background-color: $verde5
    .input_password_geral .bloco_password .bloco_verificar.incorreto:after
        content: "\e813"
        background-color: $vermelho
    /* data */
    input[data-mascara=data],
    input[data-mascara=datahora]
        width: 100%
        max-width: 140px
        padding-left: 45px
        background: url(../images/painel/calendario.png) no-repeat 3px center
    .bloco_data
        position: relative
    .bloco_data .botao
        width: 40px
        height: 40px
        line-height: 40px
        position: absolute
        top: 0
        left: 0
        color: #FFF
        background-color: $painel
        font-size: 1.5em
        text-align: center
        z-index: 2
        cursor: pointer
    .bloco_data .botao:after
        content: "\e81b"
        font-family: painel
    .bloco_data input[data-mascara=data],
    .bloco_data input[data-mascara=datahora]
        width: 100%
        background: #FFF
        padding-left: 50px
    select
        max-width: calc(100% - 20px)
        height: 40px
        position: relative
        margin: 0 20px 10px 0
        padding: 0 55px 0 10px
        border: 1px solid #DDD
        font-size: 1.3em
        background: url(../images/painel/select.png) no-repeat center right
        border-radius: 2px
    /* input cor */
    .input_cor_geral
        width: 100%
        max-width: 180px
        position: relative
        margin-bottom: 10px
    .input_cor_geral input
        width: 100%
        height: 40px
        padding: 0 40px 0 10px
        border: 1px solid #DDD
        font-size: 1.4em
        border-radius: 2px
    .input_cor_geral .botao_cor
        width: 30px
        max-width: 30px
        height: 40px
        line-height: 40px
        text-align: center
        color: #FFFFFF
        position: absolute
        top: 0
        right: 0
        padding: 0
        margin: 0
        border: 1px solid #DDD
        background-color: #CCC
        font-family: painel
        border-radius: 0
        cursor: pointer
        &::after
            content: "\e811"
/* menu mobile */
.menu_add_mobile
    width: 50px
    height: 50px
    line-height: 50px
    position: fixed
    bottom: 20px
    right: 20px
    background-color: $cor_visualizar
    color: #FFF
    border-radius: 100%
    text-align: center
    overflow: hidden
    z-index: 100
    cursor: pointer
    display: none
    &:after
        width: 50px
        height: 50px
        content: "\e807"
        font-size: 1.4em
        font-family: painel

/* bloco controlador */
.bloco_controlador
    margin-top: 10px
    display: flex
    flex-direction: row
    flex-wrap: nowrap
    .botao
        height: 40px
        line-height: 40px
        color: #FFF
        margin-left: 10px
        border-radius: 3px
        overflow: hidden
        cursor: pointer
        display: flex
    .botao.add
        background-color: $cor_add
    .botao.add i
        font-size: 1.6em
    .botao.relatorio
        background-color: $cor_relatorio
    .botao.relatorio i
        font-size: 1.4em
    .botao.visualizar
        font-size: 1.8em
        font-weight: bold
        background-color: $cor_visualizar
        display: none
    .botao.download
        background-color: $cor_download
    .botao.download i
        font-size: 1.3em
    .botao.deletar
        background-color: $cor_deletar
    .botao i
        width: 40px
        height: 40px
        text-align: center
        background-color: rgba(0,0,0,.1)
    .botao span
        height: 40px
        padding: 0 20px 0 15px
@media screen and (max-width: 900px)
    #conteudo_site.fechado .bloco_controlador .botao span
        display: none
@media screen and (max-width: 1000px)
    #conteudo_site.aberto .bloco_controlador .botao span
        display: none

#conteudo_site.mobile .bloco_controlador
    position: fixed
    right: 20px
    bottom: 20px
    &::after
        width: 50px
        height: 50px
        line-height: 50px
        color: #FFF
        text-align: center
        content: "\e807"
        font-size: 1.4em
        margin-left: 4px
        font-family: painel
        justify-content: center
        background-color: $vermelho2
        border-radius: 50%
        display: flex
        z-index: 2
        cursor: pointer
    .botao
        width: 50px
        height: 50px
        line-height: 50px
        position: absolute
        top: 0
        right: 0
        border-radius: 100%
        overflow: hidden
        margin-left: 4px
        z-index: 1
        opacity: 0
    .botao span
        width: 50px
        height: 50px
        line-height: 50px
        display: none
/* CLASS / FINAL */

/* IDS / COMECO */
/* pre visualizacao */
#bloco_previsualizacao
    fixed-mascara(9999900)
    .fechar
        width: 40px
        height: 40px
        line-height: 40px
        position: fixed
        top: 10px
        right: 20px
        text-align: center
        background-color: #FFF
        border-radius: 100%
        cursor: pointer
        z-index: 2
        &:after
            content: "\e813"
    .conteudo
        fixed-conteudo()
        background-color: #FFF
        z-index: 1

/* mascara cor */
#bloco_cor_mascara
    fixed-mascara(9999911)
    display: flex
    ul.conteudo_cor
        fixed-conteudo(390px)
        max-height: calc(100vh - 40px)
        position: relative
        background-color: #FFF
        padding: 70px 20px 20px 20px
        box-shadow: 0 0 5px #DDD
        border-radius: 4px
        overflow-x: hidden
        overflow-y: auto
        display: flex
        flex-direction: row
        flex-wrap: wrap
    ul.conteudo_cor li.cor_titulo
        width: 100%
        height: 50px
        line-height: 50px
        position: absolute
        top: 0
        left: 0
        font-size: 1.4em
        color: #666
        border-radius: 4px 4px 0 0
        padding: 0 50px 0 20px
        background-color: #EEE
    ul.conteudo_cor li.cor_titulo i
        width: 30px
        height: 30px
        line-height: 30px
        position: absolute
        top: 10px
        right: 10px
        text-align: center
        cursor: pointer
    ul.conteudo_cor li.but_cor_selecionar
        width: 48px
        height: 48px
        margin: 1px 2px 0 0
        padding: 0
        border: none
        cursor: pointer
    ul.conteudo_cor li.cor_linha
        width: 100%
        height: 1px
        background-color: #EEE
        border: none
        margin: 10px 0 8px 0

/* Voltar do topo */
#voltar_topo
    width: 50px
    height: 50px
    line-height: 50px
    position: fixed
    right: 10px
    bottom: 10px
    color: #FFF
    font-size: 1.8em
    text-align: center
    background-color: $azul
    border-radius: 100%
    opacity: .4
    cursor: pointer
    &:hover
        opacity: 1

/* botao mobile */
#botao_mobile
    width: 17px
    height: calc(100vh - 100px)
    position: fixed
    bottom: 0
    left: 0
    display: none
    z-index: 3
#conteudo_site.mobile #botao_mobile
    display: flex

/* busca principal */
#conteudo_site.aberto #busca_principal
    padding-left: 240px
#conteudo_site.fechado #busca_principal
    padding-left: 50px
#conteudo_site.aberto #busca_principal.ativo
    padding-left: 0
#conteudo_site.fechado #busca_principal.ativo
    padding-left: 0
#busca_principal
    display: flex
    flex-direction: column
    align-items: center
    &.ativo
        height: 100vh
        padding-left: 0
        z-index: 10
    form
        width: 100%
    &.ativo form
        width: calc(100% - 40px)
        max-width: 1000px
        position: relative
        display: flex
    form input
        width: 100%
        max-width: 200px
        height: 30px
        font-size: 1.2em
        color: #FFF
        margin-top: 10px
        border-bottom: 1px solid #FFF
    &.ativo form input
        max-width: 100%
        height: 50px
        margin-top: 20px
        padding-right: 40px
        font-size: 1.8em
    form input::-webkit-input-placeholder
        color: #FFF
    form button
        width: 30px
        height: 50px
        line-height: 50px
        position: absolute
        right: 0
        bottom: 3px
        color: #FFF
        font-size: 2.2em
        text-align: center
        display: none
    &.ativo form button
        display: block
    form button i
