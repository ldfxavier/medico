        display: flex
        flex-direction: column
    .tr .esquerda .td
        width: 100%
        padding: 0 10px
        overflow: hidden
    .tr .direita
        width: calc(100% - 140px)
        display: flex
    .tr .td
        height: 30px
        line-height: 30px
        font-size: 1.4em
    .tr .direita .td
        border-left: 1px solid #EEE
