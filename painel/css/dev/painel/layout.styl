        border-radius: 3px
    ul li span.valor
        width: 70px
        text-align: center

.app_geral_relatorio .bloco_grafico_lista
    width: 100%
    margin-bottom: 40px
    padding: 0 10px
    display: flex
    flex-direction: row
    .grafico_lista
        col(3)
        background-color: #FFF
        box-shadow: 0 2px 2px #CCC
        border-radius: 3px
    .grafico_lista h3
        background-color: #FFF
        border-bottom: 1px solid #CCC
    .grafico_lista ul
        max-height: 300px
        overflow-y: auto
        overflow-x: hidden
    .grafico_lista ul li span.nome
        width: calc(100% - 130px)
        max-width: calc(100% - 130px)
    .grafico_lista ul li span.porcentagem
        width: 60px

.app_geral_relatorio .grafico_quadrado
    width: 100%
    display: flex
    flex-direction: row
    flex-wrap: wrap
    .grande
        width: 50%
        display: flex
        flex-direction: column
        align-items: center
        justify-content: center
        .nome
            font-size: 1.6em
            display: flex
            flex-direction: column
            align-items: center
            justify-content: center
        .nome span
            width: 100%
            text-align: center
        .valor
            margin-top: 10px
            font-size: 3.0em
            padding: 15px 20px
            border: 1px solid #EEE
            border-radius: 3px
            cursor: default

.app_geral_relatorio .bloco_dia_acesso
    width: calc(100% - 40px)
    padding: 20px
    margin: 0 20px 40px 20px
    background-color: #FFF
    border-radius: 3px
    box-shadow: 0 1px 2px #CCC
    .hora
        width: 100%
        display: flex
        flex-direction: column
    .hora .conteudo
        width: 100%
        display: flex
    .hora .conteudo .nome
        width: 40px
        display: flex
        flex-direction: column
    .hora .conteudo .nome span
        height: 20px
        line-height: 18px
        font-size: 1.4em
        margin: 2px 0
    .hora .conteudo .campo
        width: calc(100% - 40px)
        display: flex
        flex-direction: row
        flex-wrap: wrap
    .hora .conteudo .campo .valor
        col(24, 4px)
        height: 20px
        position: relative
        margin: 2px
        background-color: #DDD
        display: flex
    .hora .conteudo .campo .valor span
        width: 100%
        height: 100%
        position: absolute
        top: 0
        left: 0
    .hora .conteudo .campo .minuto
        col(24, 4px)
        height: 20px
        line-height: 24px
        text-align: center
        font-size: 1.2em
        margin: 2px
    .hora .legenda
        width: 100%
        padding-left: 40px
        font-size: 1.2em
        margin-top: 5px
        display: flex
        flex-direction: row
        align-items: center
        justify-content: flex-start
    .hora .coluna
        width: 100%
        display: flex
        flex-direction: column
        margin-left: 5px
    .hora .coluna .bloco
        width: 100%
        display: flex
        flex-direction: row
    .hora .coluna .bloco span
        width: 70px
        height: 10px
        margin-right: 2px
    .hora .coluna .numero
        width: 100%
        display: flex
        flex-direction: row
    .hora .coluna .numero li
        width: 70px
        height: 10px
        margin-right: 2px
        display: flex
        justify-content: flex-end
    .hora .coluna .numero li span.zero
        width: 100%
        flex-grow: 1
    .grafico_quadrado
        margin-top: 40px
        padding-top: 20px
        border-top: 1px solid #EEE


.app_geral_relatorio .bloco_quadrado_topo
    width: 100%
    padding: 0 20px
    margin: 20px 0 40px 0
    display: flex
    flex-direction: row
    align-items: stretch
    .real
        width: 300px
        background-color: $cor_editar
        padding: 20px
    .real h3
        color: #FFF
        font-size: 1.4em
        margin-bottom: 5px
        font-weight: 100
    .real .numero
        width: 100%
        font-size: 6.0em
        color: #FFF
        margin-bottom: 20px
    .real p
        width: 100%
        color: #FFF
        font-size: 1.2em
        padding-bottom: 3px
        margin-bottom: 5px
        border-bottom: 1px solid rgba(255,255,255,.2)
    .real ul
        height: 180px
        overflow-x: hidden
        overflow-y: auto
    .real ul li.zero
        width: 100%
        color: #FFF
        padding: 50px 0
    .real ul li.lista
        width: 100%
        display: flex
    .real ul li.lista a
        width: 100%
        color: #FFF
        padding: 5px 0
        font-size: 1.2em
        &:hover
            background-color: rgba(255,255,255, .1)
    .quadrado
        width: calc(100% - 300px)
        display: flex
        flex-direction: row
        flex-wrap: wrap
    .quadrado.w100p
        width: 100%
        justify-content: space-between
    .quadrado .bloco
        width: calc(50% - 20px)
        margin-left: 20px
        display: flex
        flex-direction: column
        align-items: center
        justify-content: center
        color: #FFF
        border-radius: 3px
    .quadrado.w100p .bloco
        margin-left: 0
        padding: 10px 0
    .quadrado.bloco_3 .bloco
        width: calc((100% / 3) - 10px)
    .quadrado.bloco_4 .bloco
        width: calc(25% - 10px)
    .quadrado .bloco.azul
        background-color: #25B4FA
    .quadrado .bloco.verde
        background-color: #0BC78E
    .quadrado .bloco.vermelho
        background-color: #FF585E
    .quadrado .bloco.rocho
        background-color: #AE72E4
    .quadrado .bloco.total
        margin-bottom: 20px
    .quadrado .bloco.ativo
        margin-bottom: 20px
    .quadrado .bloco .numero
        font-size: 3.0em
    .quadrado .bloco .nome
        font-size: 1.4em
    .quadrado .bloco .porcentagem
        margin-top: 20px
        display: flex
        flex-direction: column
        align-items: center
        justify-content: center
    .quadrado .bloco .porcentagem .porcentagem_valor
        font-size: 1.6em
    .quadrado .bloco .porcentagem .porcentagem_barra
        width: 100%
        max-width: 100px
        height: 3px
        position: relative
        margin: 5px 0
        border-radius: 3px
        background-color: rgba(0,0,0,.2)
    .quadrado .bloco .porcentagem .porcentagem_barra span
        height: 3px
        position: absolute
        top: 0
        left: 0
        background-color: #FFF
        border-radius: 3px
    .quadrado .bloco .porcentagem .porcentagem_nome
        font-size: 1.2em


#bloco_sem_permissao
    width: 100%
