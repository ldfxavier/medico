#bloco_senha header i:hover
    background-color: #DDD

#bloco_senha .conteudo form
    width: 100%

    padding: 20px 20px 0 20px

#bloco_senha .conteudo form label
    width: 100%

    text-align: center
    font-size: 1.2em
    margin-bottom: 3px

#bloco_senha .conteudo form input
    width: 100%
    height: 40px

    font-size: 1.2em
    padding: 0 10px
    margin-bottom: 10px
    font-size: 1.4em
    border: 1px solid #CCC
    border-radius: 2px

#bloco_senha .conteudo form .input_password_geral
    width: 100%
#bloco_senha .conteudo form .input_password_geral .bloco_password
    width: 100%
    position: relative
#bloco_senha .conteudo form .input_password_geral .bloco_password input
    padding-right: 50px
#bloco_senha .conteudo form .input_password_geral .bloco_password .but_password_visualizar
    width: 40px
    height: 40px
    line-height: 40px
    position: absolute
    top: 0
    right: 0
    font-size: 1.4em
    text-align: center
    background-color: $azul
    color: #FFF
    cursor: pointer
#bloco_senha .conteudo form .input_password_geral .bloco_password .but_password_visualizar[data-password=ativo]:after
    content: "\e802"
    font-family: painel
#bloco_senha .conteudo form .input_password_geral .bloco_password .but_password_visualizar[data-password=inativo]:after
    content: "\e806"
    font-family: painel
#bloco_senha .conteudo form .input_password_geral .bloco_password .bloco_verificar
    width: 40px
    height: 40px
    line-height: 40px
    position: absolute
    top: 0
    right: 0
    font-size: 1.4em
    text-align: center
    cursor: pointer
#bloco_senha .conteudo form .input_password_geral .bloco_password .bloco_verificar:after
    width: 40px
    height: 40px
    font-family: painel
    color: #FFF
#bloco_senha .conteudo form .input_password_geral .bloco_password .bloco_verificar.correto:after
    content: "\e80b"
    background-color: $verde
#bloco_senha .conteudo form .input_password_geral .bloco_password .bloco_verificar.incorreto:after
    content: "\e813"
    background-color: $vermelho
#bloco_senha .conteudo form footer
    width: calc(100% + 40px)
    padding: 10px 20px
    margin: 10px 0 0 -20px
    background-color: #F4F4F4
    border-top: 1px solid #DDD
#bloco_senha .conteudo form footer button
    height: 40px
    line-height: 40px
    float: right
    padding: 0 30px 0 25px
    border-radius: 3px
    font-size: 1.2em
    color: #FFF
    background-color: $painel
    cursor: pointer



#bloco_bloquear
    width: 100%
    height: 100%
    position: fixed
    top: 0
    left: 0
    overflow-x: hidden
    overflow-y: auto
    background-color: rgba(0,0,0,.98)
    z-index: 9999900
    display: none

#bloco_bloquear .conteudo
    width: calc(100% - 40px)
    max-width: 300px
    margin: 20px auto
    background-color: #FFF
    border-radius: 3px
    display: table
    overflow: hidden

#bloco_bloquear header
    width: 100%
    height: 50px
    line-height: 50px
    position: relative

    background-color: #EEE

#bloco_bloquear header h1
    width: 100%

    text-align: center
    font-size: 1.5em

#bloco_bloquear .conteudo form
    width: 100%

    padding: 20px 20px 0 20px

#bloco_bloquear .conteudo form figure
    width: 100px
    height: 100px
    margin-bottom: 20px
    margin-left: calc((100% - 100px) / 2)
    background-size: cover
    background-position: center center
    border-radius: 100%

#bloco_bloquear .conteudo form input
    width: 100%
    height: 40px

    font-size: 1.2em
    padding: 0 50px 0 10px
    margin-bottom: 10px
    font-size: 1.4em
    border: 1px solid #CCC
    border-radius: 2px

#bloco_bloquear .conteudo form .input_password_geral
    width: 100%


#bloco_bloquear .conteudo form .input_password_geral .bloco_password
    width: 100%
    position: relative


#bloco_bloquear .conteudo form .input_password_geral .bloco_password .but_password_visualizar
    width: 40px
    height: 40px
    line-height: 40px
    position: absolute
    top: 0
    right: 0
    font-size: 1.4em
    text-align: center
    background-color: $azul
    color: #FFF
    cursor: pointer

#bloco_bloquear .conteudo form .input_password_geral .bloco_password .but_password_visualizar[data-password=ativo]:after
    content: "\e802"
    font-family: painel

#bloco_bloquear .conteudo form .input_password_geral .bloco_password .but_password_visualizar[data-password=inativo]:after
    content: "\e806"
    font-family: painel

#bloco_bloquear .conteudo form footer
    width: 100%

    margin-bottom: 15px

#bloco_bloquear .conteudo form footer button
    width: 100%
    height: 40px
    line-height: 40px

    border-radius: 3px
    font-size: 1.2em
    color: #FFF
    background-color: $painel
    cursor: pointer

#bloco_bloquear .conteudo form footer p.texto
    width: 100%

    text-align: center
    color: #999
    margin: 10px 0

#bloco_bloquear .conteudo form footer a
    width: 100%
    height: 40px
    line-height: 40px

    text-align: center
    border: 1px solid #DDD
    color: #999


#bloco_parceiro_auditoria_confirmar
    max-width: 500px;
    .contato_lista
        width: calc(100% - 40px)
        display: flex
        background-color: #F9F9F9
        margin: 20px 20px 0 20px
        padding: 10px 10px 0 10px
        flex-direction: column
        border: 1px solid #DDD
    select
            width:calc(100% - 40px)
            margin: 10px 0 10px 0
    .contato_lista span
        width: 100%
        font-size: 1.4em
        padding-bottom: 10px
        margin-bottom: 10px
        border-bottom: 1px solid #CCC
    .contato_lista span.bold
        margin-bottom: 3px
        padding-bottom: 0
        border-bottom: none
    .contato_lista span:last-child
        border-bottom: none
        padding-bottom: 0
    textarea
        width: calc(100% - 40px)
        height: 150px
        padding: 10px
        font-size: 1.4em
        border: 1px solid #CCC
        margin: 20px



/* RELATORIOS */
