.app_geral_relatorio .grafico_barra
    width: 100%
    display: flex
    flex-direction: column
    .grafico
        width: 100%

.app_geral_relatorio .grafico_linha
    width: 100%
    display: flex
    flex-direction: column
    .grafico
        width: 100%

.app_geral_relatorio .grafico_circulo
    width: 100%
    display: flex
    flex-direction: row
    .grafico
        width: 100%
        max-width: 400px
        margin-right: 20px
        border-right: 1px solid #EEE

.app_geral_relatorio .grafico_lista
    width: 100%
    display: flex
    flex-direction: column
    h3
        width: 100%
        height: 40px
        line-height: 40px
        position: relative
        background-color: #F4F4F4
        padding: 0 10px
        display: flex
        align-items: center
    h3 span
        flex-grow: 1
    h3 i
        width: 25px
        height: 25px
        line-height: 25px
        position: relative
        text-align: center
        border: 1px solid #EEE
        display: flex
    h3 i a
        width: 100%
        height: 100%
        position: absolute
        top: 0
        left: 0
    ul
        width: 100%
    ul li
        width: 100%
        display: flex
        align-items: center
        border-bottom: 1px solid #EEE
    ul li span
        padding: 10px
        font-size: 1.4em
    ul li span.nome
        width: calc(100% - 150px)
